#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;

int main() {
	int N, temp;
	vector<int> arr;
	cin >> N;
	for (int i = 0; i < N; i++) {
		cin >> temp;
		arr.push_back(temp);
	}

	reverse(arr.begin(), arr.end());

	for (int i = 0; i < N; i++)
		cout << arr[i] << " ";
	cout << endl;
}
